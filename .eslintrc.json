{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "prettier",
    "plugin:react/recommended"
  ],
  "env": {
    "jest": true,
    "browser": true,
    "shared-node-browser": true
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["babel"],
  "rules": {
    "object-curly-newline": 0,
    "arrow-parens": 0,
    "no-use-before-define": 0,
    "semi": [1, "always"],
    "quotes": [1, "single"],
    "eol-last": ["error", "always"],
    "no-undef": "error",
    "no-global-assign": 0,
    "no-restricted-globals": 0,
    "func-names": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-useless-return": 0,
    "consistent-return": 0,
    "prefer-rest-params": 0,
    "no-console": 0,
    "arrow-body-style": 0,
    "import/first": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "no-eval": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-alert": 0,
    "no-restricted-syntax": 0,
    "lines-between-class-members": 0,
    "no-plusplus": 0,
    "import/prefer-default-export": 0,
    "no-extend-native": 0,
    "object-shorthand": 0,
    "no-prototype-builtins": 0,
    "no-proto": 0,
    "prefer-object-spread": 0,
    "no-nested-ternary": 0,
    "no-new": 0,
    "new-cap": 0,
    "prefer-destructuring": 0,
    "no-new-wrappers": 0
  }
}
